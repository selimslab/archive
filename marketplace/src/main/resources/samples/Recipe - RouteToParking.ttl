# baseURI: http://w3c.github.io/bigiot/RouteToParking
# imports: http://big-iot.eu/core#
# imports: http://w3c.github.io/bigiot/RecipePatternModel
# imports: http://w3c.github.io/bigiot/offeringRecipeModel
# prefix: RouteToParking

@prefix bigiot: <http://big-iot.eu/core#> .
@prefix offeringRecipe: <http://w3c.github.io/bigiot/offeringRecipeModel#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix RouteToParking: <http://w3c.github.io/bigiot/RouteToParking#> .
@prefix pattern: <http://w3c.github.io/bigiot/RecipePatternModel#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix td: <http://w3c.github.io/wot/w3c-wot-td-ontology.owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

offeringRecipe:Recipe
  rdf:type owl:Class ;
  rdfs:subClassOf bigiot:Offering ;
.
<http://w3c.github.io/bigiot/RouteToParking>
  rdf:type owl:Ontology ;
  owl:imports bigiot: ;
  owl:imports <http://w3c.github.io/bigiot/RecipePatternModel> ;
  owl:imports <http://w3c.github.io/bigiot/offeringRecipeModel> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
RouteToParking:getGeoCordinates
  rdf:type bigiot:Offering ;
  schema:category schema:geoCordinatesCategory ;
  td:hasInput RouteToParking:getGeoCordinates_userAddress ;
  td:hasOutput RouteToParking:getGeoCordinates_userCoordinates ;
.
RouteToParking:getAllParkSites
  rdf:type bigiot:Offering ;
  schema:category schema:parkingLocationsCategory ;
  td:hasOutput RouteToParking:getAllParkSites_parkingLocations ;
.
RouteToParking:getNearestParkSite
  rdf:type bigiot:Offering ;
  schema:category schema:nearestLocationCategory ;
  td:hasInput RouteToParking:getNearestParkSite_userCoordinates ;
  td:hasInput RouteToParking:getNearestParkSite_parkingLocations ;
  td:hasOutput RouteToParking:getNearestParkSite_nearesrParkName ;
  td:hasOutput RouteToParking:getNearestParkSite_nearesrParkCoordinates ;
.
RouteToParking:getDirections
  rdf:type bigiot:Offering ;
  schema:category schema:directionsCategory ;
  td:hasInput RouteToParking:getDirections_userCoordinates ;
  td:hasInput RouteToParking:getDirections_nearesrParkCoordinates ;
  td:hasOutput RouteToParking:getDirections_instructions ;
.
RouteToParking:sendEmail
  rdf:type bigiot:Offering ;
  schema:category bigiot:sendEmailCategory ;
  td:hasInput RouteToParking:sendEmail_userEmail ;
  td:hasInput RouteToParking:sendEmail_nearesrParkName ;
  td:hasInput RouteToParking:sendEmail_instructions ;
.
RouteToParking:Interaction_1
  rdf:type offeringRecipe:Interaction ;
  offeringRecipe:hasIngredientFrom RouteToParking:getGeoCordinates ;
  offeringRecipe:hasIngredientInput RouteToParking:getNearestParkSite_userCoordinates ;
  offeringRecipe:hasIngredientOutput RouteToParking:getGeoCordinates_userCoordinates ;
  offeringRecipe:hasIngredientTo RouteToParking:getNearestParkSite ;
  offeringRecipe:hasOperation offeringRecipe:Create ;
.
RouteToParking:Interaction_2
  rdf:type offeringRecipe:Interaction ;
  offeringRecipe:hasIngredientFrom RouteToParking:getAllParkSites ;
  offeringRecipe:hasIngredientInput RouteToParking:getNearestParkSite_parkingLocations ;
  offeringRecipe:hasIngredientOutput RouteToParking:getAllParkSites_parkingLocations ;
  offeringRecipe:hasIngredientTo RouteToParking:getNearestParkSite ;
  offeringRecipe:hasOperation offeringRecipe:Update ;
.
RouteToParking:Interaction_3
  rdf:type offeringRecipe:Interaction ;
  offeringRecipe:hasIngredientFrom RouteToParking:getGeoCordinates ;
  offeringRecipe:hasIngredientInput RouteToParking:getDirections_userCoordinates ;
  offeringRecipe:hasIngredientOutput RouteToParking:getGeoCordinates_userCoordinates ;
  offeringRecipe:hasIngredientTo RouteToParking:getDirections ;
  offeringRecipe:hasOperation offeringRecipe:Update ;
.
RouteToParking:Interaction_4
  rdf:type offeringRecipe:Interaction ;
  offeringRecipe:hasIngredientFrom RouteToParking:getNearestParkSite ;
  offeringRecipe:hasIngredientInput RouteToParking:getDirections_nearesrParkCoordinates ;
  offeringRecipe:hasIngredientOutput RouteToParking:getNearestParkSite_nearesrParkCoordinates ;
  offeringRecipe:hasIngredientTo RouteToParking:getDirections ;
  offeringRecipe:hasOperation offeringRecipe:Update ;
.
RouteToParking:Interaction_5
  rdf:type offeringRecipe:Interaction ;
  offeringRecipe:hasIngredientFrom RouteToParking:getNearestParkSite ;
  offeringRecipe:hasIngredientInput RouteToParking:sendEmail_nearesrParkName ;
  offeringRecipe:hasIngredientOutput RouteToParking:getNearestParkSite_nearesrParkName ;
  offeringRecipe:hasIngredientTo RouteToParking:sendEmail ;
  offeringRecipe:hasOperation offeringRecipe:Update ;
.
RouteToParking:Interaction_6
  rdf:type offeringRecipe:Interaction ;
  offeringRecipe:hasIngredientFrom RouteToParking:getDirections ;
  offeringRecipe:hasIngredientInput RouteToParking:sendEmail_instructions ;
  offeringRecipe:hasIngredientOutput RouteToParking:getDirections_instructions ;
  offeringRecipe:hasIngredientTo RouteToParking:sendEmail ;
  offeringRecipe:hasOperation offeringRecipe:Update ;
.
RouteToParking:getGeoCordinates_userAddress
  rdf:type bigiot:Data ;
  bigiot:rdfType schema:PhysicalAddress ;
.
RouteToParking:getGeoCordinates_userCoordinates
  rdf:type bigiot:Data ;
  bigiot:rdfType schema:SourceCoordinates ;
.
RouteToParking:getNearestParkSite_userCoordinates
  rdf:type bigiot:Data ;
  bigiot:rdfType schema:SourceCoordinates ;
.
RouteToParking:getDirections_userCoordinates
  rdf:type bigiot:Data ;
  bigiot:rdfType schema:SourceCoordinates ;
.
RouteToParking:getNearestParkSite_nearesrParkCoordinates
  rdf:type bigiot:Data ;
  bigiot:rdfType schema:DestinationCoordinates ;
.
RouteToParking:getDirections_nearesrParkCoordinates
  rdf:type bigiot:Data ;
  bigiot:rdfType schema:DestinationCoordinates ;
.
RouteToParking:getNearestParkSite_nearesrParkName
  rdf:type bigiot:Data ;
  bigiot:rdfType xsd:string ;
.
RouteToParking:sendEmail_nearesrParkName
  rdf:type bigiot:Data ;
  bigiot:rdfType xsd:string ;
.
RouteToParking:getAllParkSites_parkingLocations
  rdf:type bigiot:Data ;
  bigiot:rdfType schema:parkingLocations ;
.
RouteToParking:getNearestParkSite_parkingLocations
  rdf:type bigiot:Data ;
  bigiot:rdfType schema:parkingLocations ;
.
RouteToParking:sendEmail_userEmail
  rdf:type bigiot:Data ;
  bigiot:rdfType bigiot:EMailAddress ;
.
RouteToParking:getDirections_instructions
  rdf:type bigiot:Data ;
  bigiot:rdfType bigiot:text ;
.
RouteToParking:sendEmail_instructions
  rdf:type bigiot:Data ;
  bigiot:rdfType bigiot:text ;
.
RouteToParking:EmailDirectionsToParking
  rdf:type offeringRecipe:Recipe ;
  schema:category schema:parking ;
  offeringRecipe:description "Offering recipe to retrieve directions to the nearest parking spot via email"^^xsd:string ;
  offeringRecipe:hasIngredient RouteToParking:getGeoCordinates ;
  offeringRecipe:hasIngredient RouteToParking:getAllParkSites ;
  offeringRecipe:hasIngredient RouteToParking:getNearestParkSite ;
  offeringRecipe:hasIngredient RouteToParking:getDirections ;
  offeringRecipe:hasIngredient RouteToParking:sendEmail ;
  offeringRecipe:hasInteraction RouteToParking:Interaction_1 ;
  offeringRecipe:hasInteraction RouteToParking:Interaction_2 ;
  offeringRecipe:hasInteraction RouteToParking:Interaction_3 ;
  offeringRecipe:hasInteraction RouteToParking:Interaction_4 ;
  offeringRecipe:hasInteraction RouteToParking:Interaction_5 ;
  offeringRecipe:hasInteraction RouteToParking:Interaction_6 ;
  td:hasInput RouteToParking:getGeoCordinates_userAddress ;
  td:hasInput RouteToParking:sendEmail_userEmail ;
.

