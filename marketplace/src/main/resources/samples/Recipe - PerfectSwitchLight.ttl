@prefix pattern: <http://w3c.github.io/bigiot/RecipePatternModel#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix td: <http://w3c.github.io/wot/w3c-wot-td-ontology.owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ssn:   <https://www.w3.org/2005/Incubator/ssn/ssnx/ssn#> .

offeringRecipe:Recipe
  rdf:type owl:Class ;
  rdfs:subClassOf bigiot:Offering ;
.
<http://w3c.github.io/bigiot/PerfectSwitchLightRecipe>
  rdf:type owl:Ontology ;
  owl:imports <http://w3c.github.io/bigiot/RecipePatternModel> ;
  owl:imports <http://w3c.github.io/bigiot/offeringRecipeModel> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.

PerfectSwitchLightRecipe:PerfectLight
        a                bigiot:Offering ;
        schema:category  schema:lightCat ;
        td:hasInput  PerfectSwitchLightRecipe:PerfectLight_input_perfectLightinput1 .

PerfectSwitchLightRecipe:PerfectSwitch_output_perfectSwitchOutput1
        a               bigiot:Data ;
        bigiot:rdfType  schema:Text .

PerfectSwitchLightRecipe:PerfectLight_input_perfectLightinput1
        a               bigiot:Data ;
        bigiot:rdfType  schema:Text .

PerfectSwitchLightRecipe:PerfectSwitch
        a                 bigiot:Offering ;
        schema:category   schema:switchCat ;
        td:hasInput   PerfectSwitchLightRecipe:PerfectSwitch_input_perfectSwitchInput1 ;
        td:hasOutput  PerfectSwitchLightRecipe:PerfectSwitch_output_perfectSwitchOutput1 .

PerfectSwitchLightRecipe:Interaction1
        a                               offeringRecipe:Interaction ;
        offeringRecipe:hasIngredientFrom
                PerfectSwitchLightRecipe:PerfectSwitch ;
        offeringRecipe:hasIngredientInput
                PerfectSwitchLightRecipe:PerfectLight_input_perfectLightinput1 ;
        offeringRecipe:hasIngredientOutput
                PerfectSwitchLightRecipe:PerfectSwitch_output_perfectSwitchOutput1 ;
        offeringRecipe:hasIngredientTo  PerfectSwitchLightRecipe:PerfectLight ;
        offeringRecipe:hasOperation     offeringRecipe:Update .

PerfectSwitchLightRecipe:PerfectSwitch_input_perfectSwitchInput1
        a               bigiot:Data ;
        bigiot:rdfType  schema:Number .

PerfectSwitchLightRecipe:PerfectSwitchLightRecipe
        a                              offeringRecipe:Recipe ;
        schema:category                schema:smartBuilding ;
        offeringRecipe:description     "PerfectSwitchLightRecipe" ;
        offeringRecipe:hasIngredient   PerfectSwitchLightRecipe:PerfectSwitch , PerfectSwitchLightRecipe:PerfectLight ;
        offeringRecipe:hasInteraction  PerfectSwitchLightRecipe:Interaction1 .

