{
	"@context" : [
		"http://w3c.github.io/wot/w3c-wot-td-context.jsonld", {
			"xsd" : "http://www.w3.org/2001/XMLSchema#",
			"schema" : "http://schema.org/",
			"bigiot" : "http://big-iot.eu/core#",
			"geo" : "http://www.w3.org/2003/01/geo/wgs84_pos#",
			"skos" : "http://www.w3.org/2004/02/skos/core#",
			"rdf" : "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
			"rdfs" : "http://www.w3.org/2000/01/rdf-schema#",
			"mobility" : "http://big-iot.eu/mobility#",
			"owl" : "https://www.w3.org/2002/07/owl#",
			"td" : "http://www.w3c.org/wot/td#",
			"xsd" : "http://www.w3.org/2001/XMLSchema#",

			"offering" : "bigiot:Offering",

			"name" : "schema:name",
			"endpoint" : "bigiot:endpoint",
			"region" : "schema:spatialCoverage",
			"price" : "schema:priceSpecification",
			"amount" : {
				"@id" : "schema:price",
				"@type" : "xsd:double"
			},
			"currency" : "schema:priceCurrency",
			"license" : {
				"@id" : "schema:license",
				"@container" : "@index"
			},
			"providerId" : "bigiot:providerId",
			"localId" : "bigiot:offeringId",
			"category" : "schema:category",

			"accessInterfaceType" : "bigiot:accessInterfaceType",
			"supportsStreaming" : "bigiot:supportsStreaming",
			"supportsSubscription" : "bigiot:supportsSubscription",
			"endpointType" : "bigiot:endpointType",
			"url" : "schema:url",

			"accounting" : "bigiot:accountingModel",

			"rdfType" : "bigiot:rdfType",

			"licenseType" : "bigiot:licenseType",

			"parking" : "schema:parking",
			"parkingSiteManagement" : "schema:parkingSiteManagement",
			"parkingSpaceManagement" : "schema:parkingSpaceManagement",
			"traffic" : "schema:traffic",
			"environment" : "schema:environment",

			"radius" : {
				"@id" : "schema:radius",
				"@type" : "xsd:int"
			},
			"lat" : {
				"@id" : "geo:lat",
				"@type" : "xsd:double"
			},
			"long" : {
				"@id" : "geo:long",
				"@type" : "xsd:double"
			}
		}
	]
