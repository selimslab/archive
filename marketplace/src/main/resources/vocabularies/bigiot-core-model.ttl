# baseURI: http://big-iot.eu/core#
# imports: http://w3c.github.io/wot/w3c-wot-td-context.jsonld
# imports: http://www.w3.org/2004/02/skos/core
# imports: http://www.w3.org/2008/05/skos-xl
# imports: https://www.w3.org/2002/07/owl#
# prefix: bigiot-core

@prefix : <http://big-iot.eu/core#> .
@prefix bigiot-core: <http://big-iot.eu/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix td: <http://www.w3c.org/wot/td#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

bigiot-core:
  rdf:type owl:Ontology ;
  owl:imports <http://w3c.github.io/wot/w3c-wot-td-context.jsonld> ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  owl:imports <http://www.w3.org/2008/05/skos-xl> ;
  owl:imports <https://www.w3.org/2002/07/owl#> ;
.

bigiot-core:Categorization
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
bigiot-core:Consumer
  rdf:type owl:Class ;
  rdfs:comment "Consumers discover and subscribe to offerings of interest via a marketplace in order to access the resources. A consumer can be either an application or service instance that requires access to IoT resources in order to implement an intended service or function (e.g., a smart parking service provided by the city)" ;
  rdfs:label "Consumer" ;
  rdfs:subClassOf owl:Thing ;
.
bigiot-core:Data
  rdf:type owl:Class ;
  rdfs:comment "Gives a detail description of the transported payload data between consumer and provider based on this offering" ;
  rdfs:subClassOf owl:Thing ;
.
bigiot-core:DataTypeAnnotation
  rdf:type owl:Class ;
  rdfs:comment "" ;
  rdfs:subClassOf owl:Thing ;
.
bigiot-core:Endpoint
  rdf:type owl:Class ;
  rdfs:comment "The endpoint includes all relevant information which is required to setup right communication to the provider's offering" ;
  rdfs:label "Endpoint" ;
  rdfs:subClassOf owl:Thing ;
.
bigiot-core:ExternalEndpoint
  rdf:type owl:Class ;
  rdfs:comment "The endpoint includes all relevant information which is required to setup right communication to the provider's offering" ;
  rdfs:label "External endpoint" ;
  rdfs:subClassOf bigiot-core:Endpoint ;
.
bigiot-core:LibEndpoint
  rdf:type owl:Class ;
  rdfs:comment "The endpoint includes all relevant information which is required to setup right communication to the provider's offering" ;
  rdfs:label "Lib endpoint" ;
  rdfs:subClassOf bigiot-core:Endpoint ;
.
bigiot-core:License
  rdf:type owl:Class ;
  rdfs:comment "License of the data provided by the offering" ;
  rdfs:label "License" ;
  rdfs:subClassOf owl:Thing ;
.
bigiot-core:Offering
  rdf:type owl:Class ;
  rdfs:comment "An offering encompasses a set of IoT resources, typically a set of related information (e.g. low-level sensor data like temperature or aggregate information across a region) or functions (e.g. actuation tasks like open a gate or computational functions like computing route), that are offered on a marketplace." ;
  rdfs:subClassOf schema:Offer ;
  rdfs:subClassOf owl:Thing ;
.
bigiot-core:OfferingCategory
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
bigiot-core:OfferingQuery
  rdf:type owl:Class ;
  rdfs:comment "" ;
  rdfs:subClassOf bigiot-core:Query ;
.
bigiot-core:OfferingSubscription
  rdf:type owl:Class ;
  rdfs:comment "" ;
.
bigiot-core:Provider
  rdf:type owl:Class ;
  rdfs:comment "Providers register their offerings on a marketplace and provides access to the offered resources via a common API. A provider can be either a platform or a service instance that offers available resources, i.e., some information or access to functions that it wants to share or trade on the marketplace (e.g. an IoT platform of a parking lot provider)" ;
  rdfs:label "Provider" ;
  rdfs:subClassOf owl:Thing ;
.
bigiot-core:ProxyEndpoint
  rdf:type owl:Class ;
  rdfs:comment "The endpoint includes all relevant information which is required to setup right communication to the provider's offering" ;
  rdfs:label "Proxy endpoint" ;
  rdfs:subClassOf bigiot-core:Endpoint ;
.
bigiot-core:Query
  rdf:type owl:Class ;
  rdfs:comment "The endpoint includes all relevant information which is required to setup right communication to the provider's offering" ;
  rdfs:label "Consumers discover offerings they're interesting through the marketplace by providing an (offering) query. The query entails a specification of the type of offerings the consumer is interested in" ;
  rdfs:subClassOf owl:Thing ;
.
bigiot-core:ac_bigiot_lib
  rdf:type owl:NamedIndividual ;
  rdfs:comment "BigIoT lib access interface type" ;
  rdfs:label "BIGIOT_LIB" ;
  skos:prefLabel "ac bigiot lib" ;
.
bigiot-core:ac_external_lib
  rdf:type owl:NamedIndividual ;
  rdfs:comment "External lib access interface type" ;
  rdfs:label "EXTERNAL" ;
  skos:prefLabel "ac external lib" ;
.
bigiot-core:accessInterfaceType
  rdf:type owl:ObjectProperty ;
  rdfs:comment "access interface type" ;
  rdfs:domain bigiot-core:Endpoint ;
  rdfs:range [
      rdf:type rdfs:Resource ;
      owl:oneOf (
          bigiot-core:ac_bigiot_lib
          bigiot-core:ac_external_lib
        ) ;
    ] ;
.
bigiot-core:allOfferings
  rdf:type bigiot-core:OfferingCategory ;
  rdfs:comment "" ;
.
bigiot-core:consumerId
  rdf:type owl:DatatypeProperty ;
  rdfs:domain bigiot-core:Consumer ;
  rdfs:range xsd:string ;
.
bigiot-core:creative_commons
  rdf:type owl:NamedIndividual ;
  rdfs:comment "Creative Commons type" ;
  rdfs:label "CREATIVE_COMMONS" ;
  skos:prefLabel "Creative Commons" ;
.
bigiot-core:endpoint
  rdf:type owl:ObjectProperty ;
  rdfs:domain bigiot-core:Offering ;
  rdfs:range bigiot-core:Endpoint ;
.
bigiot-core:endpointType
  rdf:type owl:ObjectProperty ;
  rdfs:comment "endpoint type" ;
  rdfs:domain bigiot-core:Endpoint ;
  rdfs:range [
      rdf:type rdfs:Resource ;
      owl:oneOf (
          bigiot-core:http_get
          bigiot-core:http_post
          bigiot-core:endpoint_socket
        ) ;
    ] ;
.
bigiot-core:endpoint_socket
  rdf:type owl:NamedIndividual ;
  rdfs:comment "endpoint type" ;
  rdfs:label "WEBSOCKET" ;
  skos:prefLabel "endpoint socket" ;
.
bigiot-core:expectedAnnotation
  rdf:type owl:ObjectProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:OfferingCategory ;
  rdfs:range bigiot-core:DataTypeAnnotation ;
.
bigiot-core:free_price
  rdf:type owl:NamedIndividual ;
  rdfs:comment "FREE pricing model" ;
  rdfs:label "FREE" ;
  skos:prefLabel "FREE" ;
.
bigiot-core:http_get
  rdf:type owl:NamedIndividual ;
  rdfs:comment "HTTP Get endpoint type" ;
  rdfs:label "HTTP_GET" ;
  skos:prefLabel "http get" ;
.
bigiot-core:http_post
  rdf:type owl:NamedIndividual ;
  rdfs:comment "endpoint type" ;
  rdfs:label "HTTP_POST" ;
  skos:prefLabel "http post" ;
.
bigiot-core:isActivated
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:Offering ;
  rdfs:range xsd:boolean ;
.
bigiot-core:isProvidedBy
  rdf:type owl:ObjectProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:Offering ;
  rdfs:range bigiot-core:Provider ;
.
bigiot-core:isRegisteredBy
  rdf:type owl:ObjectProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:OfferingQuery ;
  rdfs:range bigiot-core:Consumer ;
.
bigiot-core:licenseType
  rdf:type owl:DatatypeProperty ;
  rdfs:domain bigiot-core:License ;
  rdfs:range [
      rdf:type rdfs:Resource ;
      owl:oneOf (
          bigiot-core:creative_commons
          bigiot-core:non_commercial_data_lc
          bigiot-core:open_data_license
        ) ;
    ] ;
.
bigiot-core:non_commercial_data_lc
  rdf:type owl:NamedIndividual ;
  rdfs:comment "Non commercial data license" ;
  rdfs:label "NON_COMMERCIAL_DATA_LICENSE" ;
  skos:prefLabel "Non Commercial Data License" ;
.
bigiot-core:offering
  rdf:type owl:ObjectProperty ;
  rdfs:domain bigiot-core:Provider ;
  rdfs:range bigiot-core:Offering ;
.
bigiot-core:offeringExpirationTime
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "" ;
.
bigiot-core:offeringId
  rdf:type owl:DatatypeProperty ;
  rdfs:domain bigiot-core:Offering ;
  rdfs:label "offering id" ;
  rdfs:range xsd:string ;
.
bigiot-core:open_data_license
  rdf:type owl:NamedIndividual ;
  rdfs:comment "Open Data License type" ;
  rdfs:label "OPEN_DATA_LICENSE" ;
  skos:prefLabel "open data license" ;
.
bigiot-core:organizationId
  rdf:type owl:DatatypeProperty ;
  rdfs:domain schema:Organization ;
  rdfs:range xsd:string ;
.
bigiot-core:per_access_price
  rdf:type owl:NamedIndividual ;
  rdfs:comment "Per access pricing model" ;
  rdfs:label "PER_ACCESS" ;
  skos:prefLabel "Per Month" ;
.
bigiot-core:per_byte_price
  rdf:type owl:NamedIndividual ;
  rdfs:comment "Per byte pricing model" ;
  rdfs:label "PER_BYTE" ;
  skos:prefLabel "Per Byte" ;
.
bigiot-core:per_month_price
  rdf:type owl:NamedIndividual ;
  rdfs:comment "Per month pricing model" ;
  rdfs:label "PER_MONTH" ;
  skos:prefLabel "Per Month" ;
.
bigiot-core:pricingModel
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "" ;
  rdfs:domain schema:PriceSpecification ;
  rdfs:label "pricing model" ;
  rdfs:range [
      rdf:type rdfs:Resource ;
      owl:oneOf (
          bigiot-core:per_month_price
          bigiot-core:per_access_price
          bigiot-core:free_price
          bigiot-core:per_byte_price
        ) ;
    ] ;
.
bigiot-core:providerId
  rdf:type owl:DatatypeProperty ;
  rdfs:domain bigiot-core:Provider ;
  rdfs:range xsd:string ;
.
bigiot-core:queryId
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:Query ;
  rdfs:range xsd:string ;
.
bigiot-core:queryTemplate
  rdf:type owl:ObjectProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:Query ;
  rdfs:label "query template" ;
  rdfs:range rdfs:Resource ;
.






bigiot-core:rdfAnnotation
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:Data ;
  rdfs:label "rdf type" ;
  rdfs:range bigiot-core:DataTypeAnnotation ;
.


bigiot-core:rdfType
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:Data ;
  rdfs:label "rdf type" ;
  rdfs:range bigiot-core:DataTypeAnnotation ;
.

bigiot-core:valueType
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:Data ;
  rdfs:label "value type" ;
  rdfs:range schema:DataType ;
.

bigiot-core:unit
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:Data ;
  rdfs:label "quantity unit type" ;
  rdfs:range bigiot-core:QuantityUnit ;
.






bigiot-core:refersTo
  rdf:type owl:ObjectProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:OfferingCategory ;
  rdfs:label "PER_ACCESS" ;
  rdfs:range rdfs:Class ;
.
bigiot-core:registerOfferingQuery
  rdf:type owl:ObjectProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:Consumer ;
  rdfs:range bigiot-core:OfferingQuery ;
.
bigiot-core:subscribeTo
  rdf:type owl:ObjectProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:OfferingSubscription ;
  rdfs:range bigiot-core:Offering ;
.
bigiot-core:subscribedQuery
  rdf:type owl:ObjectProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:OfferingSubscription ;
  rdfs:range bigiot-core:OfferingQuery ;
.
bigiot-core:subscriptionId
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:OfferingSubscription ;
  rdfs:range xsd:string ;
.
schema:category
  rdfs:domain bigiot-core:Offering ;
  rdfs:range bigiot-core:OfferingCategory ;
.
schema:license
  rdfs:domain bigiot-core:Offering ;
  rdfs:domain bigiot-core:Provider ;
  rdfs:domain schema:Organization ;
  rdfs:range bigiot-core:License ;
.
schema:name
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "" ;
  rdfs:domain owl:Thing ;
  rdfs:range xsd:string ;
.
schema:priceCurrency
  rdfs:range xsd:string ;
.
schema:priceSpecification
  rdfs:domain bigiot-core:Offering ;
  rdfs:domain bigiot-core:Provider ;
  rdfs:domain schema:Organization ;
.
schema:sourceOrganization
  rdfs:domain bigiot-core:Consumer ;
  rdfs:domain bigiot-core:Provider ;
.
skos:topConceptOf
  rdfs:domain bigiot-core:OfferingCategory ;
  rdfs:range bigiot-core:Categorization ;
.
td:hasInput
  rdf:type owl:ObjectProperty ;
  rdfs:domain bigiot-core:Offering ;
  rdfs:range bigiot-core:Data ;
.
td:hasOutput
  rdf:type owl:ObjectProperty ;
  rdfs:comment "" ;
  rdfs:domain bigiot-core:Offering ;
  rdfs:label "output data" ;
  rdfs:range bigiot-core:Data ;
.
