# General "non-functional concepts"

schema:sensorID
    rdfs:label "SensorIdentifier" ;
    schema:domainIncludes bigiot-core:System  ;
    schema:rangeIncludes schema:Text ;
    rdfs:comment "Identifier ID of Sensor" .

schema:accuracy
    rdfs:label "SystemAccuracy" ;
    schema:domainIncludes bigiot-core:System  ;
    schema:rangeIncludes schema:Number ;
    schema:rangeIncludes schema:Text ;
    rdfs:comment "Accuracy value for the sensor/system" .

