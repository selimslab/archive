			"traffic": "schema:traffic",
			"environment": "schema:environment",

			"radius": {
				"@id": "schema:radius",
				"@type": "xsd:int"
			},
			"lat": {
				"@id": "geo:lat",
				"@type": "xsd:double"
			},
			"long": {
				"@id": "geo:long",
				"@type": "xsd:double"
			}
