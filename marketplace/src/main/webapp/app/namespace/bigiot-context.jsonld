			},
			"events": {
				"@id": "td:hasEvent",
				"@container": "@set"
			},
			"Event": "td:Event",
			"property": {
				"@id": "td:forProperty",
				"@type": "@id"
			},
			"inputData": {
				"@id": "td:hasInput"
			},
			"outputData": {
				"@id": "td:hasOutput"
			},
			"actions": {
				"@id": "td:hasAction",
