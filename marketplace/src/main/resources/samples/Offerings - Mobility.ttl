<http://w3c.github.io/bigiot/OSMGeoCordinatesOffering>
        a                          bigiot:Offering ;
        bigiot:endpoint            [
                bigiot:accessInterfaceType  "BIGIOT_LIB" ;
                bigiot:endpointType "HTTP_GET" ;
                schema:url                  "http://localhost:8080/marketplace/gateway/getGeoCordinates"
                ] ;
        bigiot:providerId          "SP-NG-01" ;
        td:hasInput
                [ bigiot:rdfType  schema:PhysicalAddress ;
                  schema:name     "userAddress"
                ] ;
        td:hasOutput
                [ bigiot:rdfType  schema:SourceCoordinates ;
                  schema:name     "GeoCoordinates"
                ] ;
        schema:category            schema:geoCordinatesCategory ;
        schema:license             "OPEN_DATA_LICENSE" ;
        schema:name                "Open Street Maps" ;
        schema:priceSpecification  [ bigiot:accountingModel  "PER_ACCESS" ;
                                     schema:price            "0.002000"^^xsd:double ;
                                     schema:priceCurrency    "EUR"
                                   ] ;
        schema:spatialCoverage     "Berlin" .

