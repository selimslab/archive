				"@id": "td:isAssociatedTo",
				"@type": "@id",
				"@container": "@set"
			},
			"links": {
				"@id": "td:hasInteraction",
				"@type": "@id",
				"@container": "@set"
			},
			"priority": {
				"@id": "td:hasPriority",
