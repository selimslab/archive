# baseURI: http://w3c.github.io/bigiot/RecipePatternModel
# imports: http://w3c.github.io/bigiot/offeringRecipeModel
# prefix: pattern

@prefix bigiot-core: <http://big-iot.eu/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pattern: <http://w3c.github.io/bigiot/RecipePatternModel#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://w3c.github.io/bigiot/RecipePatternModel>
  rdf:type owl:Ontology ;
  owl:imports <http://w3c.github.io/bigiot/offeringRecipeModel> ;
.
pattern:If_Condition
  rdf:type owl:Class ;
  rdfs:subClassOf pattern:RecipePattern ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom bigiot-core:Data ;
      owl:onProperty pattern:hasVariable ;
    ] ;
.
pattern:Loop
  rdf:type owl:Class ;
  rdfs:subClassOf pattern:RecipePattern ;
.
pattern:RecipePattern
  rdf:type owl:Class ;
.
pattern:RelationalOperator
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
pattern:equalTo
  rdf:type pattern:RelationalOperator ;
.
pattern:greaterThan
  rdf:type pattern:RelationalOperator ;
.
pattern:greaterThanOrEqualTo
  rdf:type pattern:RelationalOperator ;
.
pattern:hasCondition
  rdf:type owl:ObjectProperty ;
.
pattern:hasRelationalOperator
  rdf:type owl:ObjectProperty ;
  rdfs:range pattern:RelationalOperator ;
.
pattern:hasValue
  rdf:type owl:DatatypeProperty ;
  rdfs:domain pattern:If_Condition ;
.
pattern:hasVariable
  rdf:type owl:ObjectProperty ;
.
pattern:lessThan
  rdf:type pattern:RelationalOperator ;
.
pattern:lessThanOrEqualTo
  rdf:type pattern:RelationalOperator ;
.
pattern:notEqualTo
  rdf:type pattern:RelationalOperator ;
.
